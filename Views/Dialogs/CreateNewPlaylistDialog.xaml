<?xml version="1.0" encoding="utf-8"?>
<ContentDialog
    x:Class="OneLastSong.Views.Dialogs.CreateNewPlaylistDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:OneLastSong.Views.Dialogs"
    xmlns:l="using:WinUI3Localizer"
    PrimaryButtonText="Ok"
    SecondaryButtonText="Cancel"
    PrimaryButtonClick="ContentDialog_PrimaryButtonClick"
    SecondaryButtonClick="ContentDialog_SecondaryButtonClick"
    l:Uids.Uid="New_Playlist_ContentDialog"
    Background="{ThemeResource BG_PRIMARY}"
    Foreground="{ThemeResource TEXT_PRIMARY}"
    CornerRadius="13"
    >

    <StackPanel>
        <TextBox x:Name="PlaylistNameTextBox" l:Uids.Uid="Enter_playlist_name_TextBox" Foreground="{ThemeResource TEXT_PRIMARY}"/>
    </StackPanel>
</ContentDialog>
